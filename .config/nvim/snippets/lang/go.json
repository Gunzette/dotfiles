{
	"pass error downstream": {
		"prefix": "ifErr",
		"body": "if err != nil {\n\treturn ${1:\"\"}, err\n}",
		"description": "Snippet to pass through errors"
	},
	"for i statement": {
		"prefix": "fori",
		"body": "for ${1:i} := ${2:0}; $1 < ${3:bound}; $1${4:++} {\n\t$0\n}",
		"description": "Simple counting for loop"
	}
}
